<p id="notice"><%= notice %></p>
<table class = "table table-striped table-bordered table-condensed ">
<thead>
  <strong>Title:</strong>
  <%= @subcategory.title %>
</thead>
<tbody >
	<% @subcategory.bookmarks.each do |bookmark| %>
	<tr>
	<td><%= bookmark.title %></td>
	<td><%= link_to 'Show', bookmark ,:class => "btn btn-info"%></td>
	</tr>
	<% end %>
</tbody>
</table>
<br>

<%= link_to 'Edit', edit_subcategory_path(@subcategory) , :class => "btn btn-warning" %> |
<%= link_to 'Back', subcategories_path  , :class => "btn btn-default"%>
